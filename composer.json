{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "*",
        "dedoc/scramble": "^0.8.6",
        "emreyarligan/enum-concern": "^1.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "hammerstone/fast-paginate": "^1.1",
        "laravel/framework": "^10.48",
        "laravel/horizon": "^5.24",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "opcodesio/log-viewer": "^3.10",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/laravel-medialibrary": "^11.6",
        "spatie/laravel-query-builder": "^6.0",
        "spatie/laravel-settings": "^3.3",
        "symfony/filesystem": "^7.1",
        "ext-zip": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.9",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.29",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "plannr/laravel-fast-refresh-database": "^1.2",
        "spatie/laravel-ignition": "^2.7",
        "spatie/laravel-ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "echo Adding Git hooks && git config core.hooksPath .hooks"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan log-viewer:publish",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
